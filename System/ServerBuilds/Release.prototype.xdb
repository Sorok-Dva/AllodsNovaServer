<?xml version="1.0" encoding="UTF-8" ?>
<basement.server.config.ServerBuild>
	<Header>
		<resourceId>107439119</resourceId>
		<isPrototype>true</isPrototype>
	</Header>
	<oneThread>false</oneThread>
	<forceGeometryIntersection>true</forceGeometryIntersection>
	<restrictedMapsLoading>false</restrictedMapsLoading>
	<checkLocaleReferences>false</checkLocaleReferences>
	<isIndexUploadEnabled>false</isIndexUploadEnabled>
	<cloneMsgsToOtherServers>false</cloneMsgsToOtherServers>
	<saveAvatarIntervalMS>900000</saveAvatarIntervalMS>
	<checkBunches>false</checkBunches>
	<shardNetBuffSize>10000</shardNetBuffSize>
	<clientStatsPeriodSeconds>600</clientStatsPeriodSeconds>
	<fixAvatarPositionOnEnter>true</fixAvatarPositionOnEnter>
	<revealAggroLists>true</revealAggroLists>
	<loadLockTreshold>30</loadLockTreshold>
	<lazyResourceLoading>false</lazyResourceLoading>
	<loadMapStaticsCache>true</loadMapStaticsCache>
	<debugBuild>false</debugBuild>
	<forceLoadDirs>
		<Item>/</Item>
	</forceLoadDirs>
    <clientNetTimeout>-1</clientNetTimeout>
	<msgQueueSettings>
		<localMsgQueueSize>512000</localMsgQueueSize>
		<defaultSize>16000</defaultSize>
		<server2server>
			<Item>
				<from>FrontEnd</from>
				<to>GameMechanics</to>
				<size>256000</size>
			</Item>
			<Item>
				<from>GameMechanics</from>
				<to>FrontEnd</to>
				<size>256000</size>
			</Item>
			<Item>
				<from>ItemService</from>
				<to>GameMechanics</to>
				<size>256000</size>
			</Item>
			<Item>
				<from>GameMechanics</from>
				<to>GameMechanics</to>
				<size>256000</size>
			</Item>
		</server2server>
		<server2all>
			<Item>
				<from>FrontEnd</from>
				<size>32000</size>
			</Item>
			<Item>
				<from>GameMechanics</from>
				<size>32000</size>
			</Item>
			<Item>
				<from>ItemService</from>
				<size>32000</size>
			</Item>
		</server2all>
	</msgQueueSettings>

	<bufferSettings>
		<defaultOutBufferSize>1000000</defaultOutBufferSize>
		<defaultInBufferSize>1000000</defaultInBufferSize>
		<connections>
			<Item>
				<from>FrontEnd</from>
				<to>GameMechanics</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>10000000</inBufferSize>
			</Item>
			<Item>
				<from>GameMechanics</from>
				<to>FrontEnd</to>
				<outBufferSize>10000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>ItemService</from>
				<to>GameMechanics</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>GameMechanics</from>
				<to>ItemService</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>Transfer</from>
				<to>Transfer</to>
				<outBufferSize>20000000</outBufferSize>
				<inBufferSize>20000000</inBufferSize>
			</Item>
			<Item>
				<from>FrontEnd</from>
				<to>ItemService</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>ItemService</from>
				<to>FrontEnd</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>FrontEnd</from>
				<to>Pathfinder</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
			<Item>
				<from>Pathfinder</from>
				<to>FrontEnd</to>
				<outBufferSize>1000000</outBufferSize>
				<inBufferSize>1000000</inBufferSize>
			</Item>
		</connections>
	</bufferSettings>

	<turnOnProbes>
		<!-- IMPORTANT probes, modifications must be approved by V.Zlobin -->
		<Item>System.Alarms</Item>
		<!-- /IMPORTANT probes, modifications must be approved by V.Zlobin -->
		<Item>lag</Item>
		<Item>loadCollector</Item>
		<Item>fps</Item>
        <Item>memory</Item>
		<Item>cmd</Item>
	</turnOnProbes>
	<turnOffProbes>
	</turnOffProbes>
</basement.server.config.ServerBuild>
